{
  "timeline": [
    {
      "type": "sequence",
      "condition": "q.do_effect_walks",
      "keyframes": [
        "save_position",
        "move_to_target",
        "pause"
      ]
    },
    {
      "type": "animation",
      "animation": ["scratch", "physical"],
      "variables": [
        "v.main_sound='cobblemon:scratch'",
        "v.main_particle[0]='cobblemon:scratch'"
      ],
      "delay": "q.do_effect_walks ? 3 : 1"
    },
    {
      "type": "sequence",
      "condition": "q.do_effect_walks",
      "keyframes": [
        "return_to_position",
        "pause"
      ]
    }
  ]
}